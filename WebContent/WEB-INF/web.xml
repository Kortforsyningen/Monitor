		<display-name>ServiceApiGroupStatController</display-name>
		<servlet-name>ServiceApiGroupStatController</servlet-name>
		<servlet-class>
		org.easysdi.monitor.gui.webapp.controllers.ServiceApiGroupStatController</servlet-class>
	</servlet>
	<servlet>
