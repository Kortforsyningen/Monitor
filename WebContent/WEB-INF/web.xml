		<url-pattern>/RequestRawLogExportController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GroupRawlogStatExportController</servlet-name>
		<url-pattern>/GroupRawlogStatExportController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServiceApiGroupStatController</servlet-name>
		<url-pattern>/ServiceApiGroupStatController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GroupAggHourLogExportController</servlet-name>
